{
  "version": 2,
  "buildCommand": "./vercel-build.sh",
  "outputDirectory": "client/build",
  "installCommand": "echo 'No root dependencies to install'",
  "framework": "create-react-app",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 